    />
    <meta name="viewport" content="width=device-width" />
    <link rel="icon" type="image/svg+xml" href="/favicon.ico" />
    <link rel="canonical" href="https://cesiaborjon.com/" />
    <meta name="generator" content={Astro.generator} />
    <title>{title}</title>
    <meta
      name="keywords"
      content="Ginecologa tijuana zona rio, Ginecologa tijuana precios, Los mejores ginecólogos en Tijuana, Atencion ginecologica  "
    />
    <meta name="author" content="Doctora Cesia Borjón" />
    <meta name="publisher" content="Ecommetrica" />
    <meta name="robots" content="index, follow" />
    <meta property="og:type" content="website" />
    <meta property="og:url" content="https://cesiaborjon.com/" />
    <meta property="og:title" content={title} />
    <meta
      property="og:description"
      content="Ginecoobstetra vanguardista, experta en salud sexual, reproductiva,colposcopia,patología vulvar y trastornos hormonales con 15 años de experiencia."
    />
    <meta
      property="og:image"
      content="https://cesiaborjon.com/FotoCesiaLink.webp"
    />
